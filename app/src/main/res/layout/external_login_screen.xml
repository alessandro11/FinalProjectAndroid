<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_green">



    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true">

    <ImageButton
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:id="@+id/FBbutton"
        android:background="@drawable/facebook"
        android:layout_row="0"
        android:layout_column="0" />

    <ImageButton
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:id="@+id/GPbutton"
        android:background="@drawable/google_plus"
        android:layout_row="0"
        android:layout_column="2" />

    <ImageButton
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:id="@+id/TWbutton"
        android:background="@drawable/twitter"
        android:layout_row="1"
        android:layout_column="0" />

    <ImageButton
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:id="@+id/LDbutton"
        android:background="@drawable/linkedin"
        android:layout_row="1"
        android:layout_column="2" />

    <ImageButton
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:id="@+id/FLbutton"
        android:background="@drawable/flickr"
        android:layout_row="2"
        android:layout_column="0" />

    <ImageButton
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginTop="10dp"
        android:id="@+id/YHbutton"
        android:background="@drawable/yahoo"
        android:layout_row="2"
        android:layout_column="2"
        android:layout_marginRight="10dp" />


    </GridLayout>
</RelativeLayout>